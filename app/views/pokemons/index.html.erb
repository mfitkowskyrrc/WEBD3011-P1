<p style="color: green"><%= notice %></p>

<% content_for :title, "Pokemon" %>

<%= paginate @pokemons %>

<div id="pokemons">
  <% @pokemons.each do |pokemon| %>
    <%= render pokemon %>
    <p>
      <%= link_to "Show this pokemon", pokemon %>
    </p>
  <% end %>
</div>

<%= paginate @pokemons %>

<%= link_to "New pokemon", new_pokemon_path %>
